FEED ESTABLISHMENTS
<div>

     <button type="submit" class="raise"><a href="/create-establishment">Create establishment</a></button>

</div>

<div class="row row-cols-1 row-cols-md-3 g-4 d-flex flex-wrap">
    {{#each establishments as |establishment|}}
    <div class="col">
        <div class="card h-100">
        <img src="https://mdbcdn.b-cdn.net/img/new/standard/city/044.webp" class="card-img-establishments"/>
        <div class="card-body">
            <h5 class="card-title">{{establishment.name}}</h5>
            <a class="me-auto" href="/establishments/{{establishment.id}}">Detail</a>
        </div>
        <div class="card-footer">
            <small class="text-muted">Last updated 3 mins ago</small> <!-- TODO: Formula fecha con Dayjs o Cloudinary -->
        </div>
        </div>
    </div>
    {{/each}}
</div>